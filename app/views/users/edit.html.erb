<%= form_for @user do |f| %>
<%= f.label :user_name %>
<%= f.text_field :user_name, :placeholder => "User name" %> <br>
<br>
<%= f.label :email %>
<%= f.email_field :email, :placeholder => "email" %> <br>
<br>
<%= f.label :password %>
<%= f.password_field :password %> <br>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %> <br>
    
<%= f.submit %>
<% end %>
<br>
<%= link_to 'Back to your profile page', user_path(current_user.id) %>