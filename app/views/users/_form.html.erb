<%= form_for @user do |f| %>

    <%= f.label :full_name %>
    <%= f.text_field :full_name %>
    <br>
    <%= f.label :user_name %>
    <%= f.text_field :user_name %> <br>
    <br>
    <%= f.label :email %>
    <%= f.email_field :email %> <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %> <br>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %> <br>

    <%= f.submit %>
<% end %>