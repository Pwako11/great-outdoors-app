<%= form_for @review do |f| %>
    
    <br>
    <%= f.label :title%>
    <%= f.text_area :title%><br>
    <br>
    <%= f.label :content %>
    <%= f.text_area :content %><br>
    <br>
    <%= f.label :rating %>
    <%= f.number_field :rating,  max:5, min:0  %><br>
    <br>
    <%= f.collection_select :park_id, Park.all, :id, :name %>
    <br>
    <%= f.hidden_field :user_id, :value => current_user.id %>
   
    <%= f.submit %>

  <% end %>
  